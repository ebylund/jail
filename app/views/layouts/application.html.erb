
<!DOCTYPE html>
<html>
<head>
  <title>Utah Jailbirds</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

</head>
<body ng-app="jailBirds">


  <nav class="navbar navbar-fixed-top" role="navigation">
    
      <div class="container-fluid">



        
        <div class="navbar-header">
          <button type="button" class="btn btn-navbar navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>

          <a class="navbar-brand" href="#"><%= image_tag 'Jailbird.png', width:"90", height:"68" %> Utah Jailbirds</a>
        </div>

        <div id="navbar-collapse"class="collapse navbar-collapse pull-right">
          <ul class="nav navbar-nav">
            <li id="collapsed-search">
              <form class="navbar-form navbar-right" role="search">
                <div class="form-group">
                  <input type="text" class="form-control" placeholder="Search">
                  <button type="submit" class="btn btn-default">Submit</button>
                </div>
              </form>
            </li>

          </ul>
        </div>

      </div>
    
  </nav>

  <!-- Twitter Link -->
  <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="https://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script>


  <div id="backgroundFixed">

  </div>

  <div class="container">

    <div class="row">

      <div class="span12">
        <%= yield %>
      </div>

    </div>
  </div>


</body>
</html>
