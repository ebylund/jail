<!-- <%=@paraElement%> -->

<% @personTable.each do |h| %> 

	<div><%=  h[:name] %></div>
	<div><%=  h[:address] %></div>
	<div><%=  h[:city] %></div>
	<div><%=  h[:number] %></div>
	<div><%=  h[:date] %></div>
	<div><%=  h[:officer] %></div>
	<div><%=  h[:agency] %></div>
	<div><%=  raw h[:charge] %></div>
	<div><%=  raw h[:status] %></div>
	
	
<% end %>

